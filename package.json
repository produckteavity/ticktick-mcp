{
  "name": "ticktick-mcp-server",
  "version": "0.1.0",
  "type": "module",
  "description": "Connect AI assistants to your TickTick tasks via Model Context Protocol (MCP)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/produckteavity/ticktick-mcp.git"
  },
  "homepage": "https://github.com/produckteavity/ticktick-mcp#readme",
  "keywords": [
    "mcp",
    "mcp-server",
    "ticktick",
    "model-context-protocol",
    "tasks",
    "task-management",
    "ai",
    "llm",
    "claude",
    "productivity",
    "todo"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "auth": "node dist/auth-cli.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:mcp": "vitest run tests/mcp",
    "precommit": "npm audit --audit-level=high && vitest run"
  },
  "bin": {
    "ticktick-mcp-server": "dist/index.js",
    "ticktick-mcp-server-auth": "dist/auth-cli.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.26.0",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@types/node": "25.3.0",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  }
}
